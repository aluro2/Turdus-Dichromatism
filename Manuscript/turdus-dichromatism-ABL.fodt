   <text:p text:style-name="P14"/>
   <text:p text:style-name="P4"><text:span text:style-name="T6">*correspondence: </text:span><text:a xlink:type="simple" xlink:href="mailto:alec.b.luro@gmail.com" text:style-name="Internet_20_link" text:visited-style-name="Visited_20_Internet_20_Link"><text:span text:style-name="T6">alec.b.luro@gmail.com</text:span></text:a></text:p>
